{
  "name": "@nem035/agenteval",
  "version": "0.1.0",
  "description": "A Vitest-like CLI for AI agent evaluations. Test your LLM apps with simple, declarative evals.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agenteval": "./bin/agenteval.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm run test:run"
  },
  "keywords": [
    "ai",
    "eval",
    "evals",
    "evaluation",
    "testing",
    "llm",
    "agent",
    "anthropic",
    "openai",
    "claude",
    "gpt",
    "cli",
    "vitest",
    "test"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nem035/agenteval.git"
  },
  "bugs": {
    "url": "https://github.com/nem035/agenteval/issues"
  },
  "homepage": "https://github.com/nem035/agenteval#readme",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "ai": "^6.0.0",
    "commander": "^13.0.0",
    "glob": "^11.0.0",
    "picocolors": "^1.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
